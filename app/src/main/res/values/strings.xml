    <!-- About Screen -->
    <string name="app_description">OneVault is a secure, local-first Android application that helps you manage your financial bills, store passwords safely, and organize your important files. All your data is stored locally on your device to ensure maximum privacy and security.</string>
    <string name="key_features">Key Features</string>
    <string name="feature_bill_tracker">Bill Tracker: Track and manage your bills with OCR scanning capability</string>
    <string name="feature_password_manager">Password Manager: Securely store and manage your credentials</string>
    <string name="feature_file_vault">File Vault: Organize and store your important documents</string>
    <string name="feature_biometric_security">Biometric Security: Protect your app with fingerprint or face authentication</string>
    <string name="feature_data_encryption">Advanced encryption for all your data</string>
    <string name="developer_info">Developer Information</string>
    <string name="developer_name">Adil Studio</string>
    <string name="developer_email">support@adilstudio.com</string>

    <!-- Privacy Policy Screen -->
    <string name="privacy_introduction_title">Introduction</string>
    <string name="privacy_introduction_content">OneVault is committed to protecting your privacy. This policy explains how we handle your data and protect your personal information.</string>
    <string name="privacy_information_collect_title">Information We Collect</string>
    <string name="privacy_information_collect_content">OneVault stores all data locally on your device. We do not collect or transmit any personal information to external servers. Your bills, passwords, and files remain entirely on your device.</string>
    <string name="privacy_how_we_use_title">How We Use Your Information</string>
    <string name="privacy_how_we_use_content">All data processing occurs locally on your device. We use your information solely to provide the app\'s functionality: organizing bills, managing passwords, and storing files securely.</string>
    <string name="privacy_data_security_title">Data Security</string>
    <string name="privacy_data_security_content">We implement industry-standard encryption to protect your data. All sensitive information is encrypted using AES-256 encryption before being stored on your device.</string>
    <string name="privacy_data_storage_title">Data Storage</string>
    <string name="privacy_data_storage_content">Your data is stored exclusively in your device\'s secure storage. We do not use cloud storage or external databases. You have full control over your data at all times.</string>
    <string name="privacy_third_party_title">Third-Party Services</string>
    <string name="privacy_third_party_content">OneVault may use Google ML Kit for text recognition during bill scanning. This processing occurs on-device and no data is sent to Google servers.</string>
    <string name="privacy_your_rights_title">Your Rights</string>
    <string name="privacy_your_rights_content">You have complete control over your data. You can export, delete, or modify any information stored in the app at any time. No third parties have access to your data.</string>
    <string name="privacy_contact_title">Contact Us</string>
    <string name="privacy_contact_content">If you have any questions about this privacy policy, please contact us at support@adilstudio.com</string>
    <string name="privacy_last_updated">Last updated: August 2025</string>

